    +(function($){
        $(document)
        .on('click', '#modal-lostpwd input.user-submit', function(e){
            e.preventDefault();

            var url = $('#modal-lostpwd form').attr('action');
            var data = {
                reset_password: true,
                email: $('#modal-lostpwd [name="email"]').val(),
            };
