<script defer>
    function menuTrick() {
        /* This functions moves the elements of the menu to a provisory burger for resolutions 992 to 1270 */
        var menuHeader
        var totalWidthAllowed = $(".menu_bar").outerWidth(true) - 306 - $("#search-top").outerWidth() - $("#myTopnavProvisory").outerWidth() - 50 - $('#info-bolt').outerWidth();
        var burgerVisible = 0;

        var curentTotalWidth = 0;
        $("#myTopnav li:not(#menu-item-3600)").each(function(index){
            curentTotalWidth += $(this).outerWidth();
            if (curentTotalWidth > totalWidthAllowed) {
                if (burgerVisible == 0) {
                    $("#myTopnavProvisory").removeClass("hidden-provisory");
                    burgerVisible = 1;
                    $("#myTopnav li").eq(index-1).addClass("hidden-li-menu");
                    $("#menu-headermenuaudio-1 li").eq(index-1).addClass("visible-resp-m2");
                }
                $(this).addClass("hidden-li-menu");
                $("#menu-headermenuaudio-1 li").eq(index).addClass("visible-resp-m2");
            }
            else {
                if (burgerVisible = 1) {
                    $("#myTopnavProvisory").addClass("hidden-provisory");
                    burgerVisible = 0;
                    $("#myTopnav li").eq(index-1).removeClass("hidden-li-menu");
                    $("#menu-headermenuaudio-1 li").eq(index-1).removeClass("visible-resp-m2");
                }
                $(this).removeClass("hidden-li-menu");
                $("#menu-headermenuaudio-1 li").eq(index).removeClass("visible-resp-m2");
            }
        })
    }
    $(window).resize(function(){
        menuTrick();
    })
    document.addEventListener('DOMContentLoaded', function(){
        menuTrick();
        $("#menu-headertoprightmenu2").removeClass("hide-menu-top");
    });
</script>
